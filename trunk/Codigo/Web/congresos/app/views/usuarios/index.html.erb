<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Email</th>
      <th>Fecha de nacimiento</th>
      <th>Domicilio</th>
      <th>Telefono</th>
      <th>Dni</th>
      <th>Piso</th>
      <th>Departamento</th>
      <th>Legajo</th>
      <th>Activo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |user| %>
      <tr>
        <td><%= user.nombre %></td>
        <td><%= user.apellido %></td>
        <td><%= user.email %></td>
        <td><%= user.fecha_nacimiento %></td>
        <td><%= user.domicilio %></td>
        <td><%= user.telefono %></td>
        <td><%= user.dni %></td>
        <td><%= user.piso %></td>
        <td><%= user.departamento %></td>
        <td><%= user.legajo %></td>
        <td><%= user.activo %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_usuario_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Usuario', new_usuario_path %>
