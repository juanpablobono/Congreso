<p id="notice"><%= notice %></p>

<h1>Listing Enrollments</h1>

<table>
  <thead>
    <tr>
      <th>Fecha alta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inscripciones.each do |inscripcion| %>
      <tr>
        <td><%= inscripcion.fecha_alta %></td>
        <td><%= link_to 'Show', inscripcion %></td>
        <td><%= link_to 'Edit', edit_inscripcion_path(inscripcion) %></td>
        <td><%= link_to 'Destroy', inscripcion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enrollment', new_inscripcion_path %>
