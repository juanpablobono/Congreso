<%= form_for(@inscripcion) do |f| %>
  <% if @inscripcion.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@inscripcion.errors.count, "error") %> no se puede guardar esta inscripcion:</h2>

      <ul>
      <% @inscripcion.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :fecha_alta %><br>
    <%= f.text_field :fecha_alta %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
